{
  "version": 2,
  "builds": [
    {
      "src": "ecoride/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.x"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "ecoride/wsgi.py" }],
  "env": {
    "DJANGO_ENV": "production"
  },
  "buildCommand": "bash build_files.sh"
}